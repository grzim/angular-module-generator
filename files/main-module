var --namespace-- = {};

(function(namespace){
    'use strict';
    var projectModules = {
        externalModules: [
        ],
        internalModules: [
        ],
        get: function () {
            return this.externalModules.concat(this.internalModules);
        }
    };
    namespace.app = angular.module('--app-name--', projectModules.get());
})(--namespace--);
